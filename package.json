{
  "name": "@api-typed/framework",
  "version": "0.0.1",
  "private": true,
  "description": "Opinionated TypeScript framework built on the shoulders of giants.",
  "author": {
    "name": "Michal Palys-Dudek",
    "email": "michal@pnd.io"
  },
  "license": "MIT",
  "homepage": "https://github.com/api-typed/framework#readme",
  "bugs": "https://github.com/api-typed/framework/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/api-typed/framework.git"
  },
  "keywords": [
    "api",
    "typescript",
    "routing-controllers",
    "typeorm",
    "di",
    "typedi",
    "framework"
  ],
  "bin": {
    "api-typed": "packages/api-typed/bin/api-typed.js"
  },
  "scripts": {
    "bootstrap": "lerna exec rimraf node_modules && lerna bootstrap --hoist && lerna link --force-local",
    "build": "lerna run build",
    "clear": "lerna exec rimraf node_modules && lerna exec rimraf dist",
    "dev": "lerna run --stream --parallel dev",
    "lint": "lerna run lint",
    "prerelease": "npm run build && npm run lint && npm run test && npm run test:e2e",
    "release": "lerna publish",
    "reset": "npm run clear && npm run bootstrap",
    "test:app": "API_TYPED_DEV_DIRS=packages api-typed run http-dev",
    "test:app:worker": "API_TYPED_DEV_DIRS=packages api-typed run worker-dev",
    "test:db": "docker-compose up",
    "test:coverage": "jest --runInBand --coverage",
    "test:e2e:coverage": "jest --runInBand --config jest.e2e.config.ts --coverage",
    "test:e2e": "jest --runInBand --config jest.e2e.config.ts",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "@api-typed/app": "file:packages/app",
    "@api-typed/command-line": "file:packages/command-line",
    "@api-typed/common": "file:packages/common",
    "@api-typed/config": "file:packages/config",
    "@api-typed/http-module": "file:packages/http-module",
    "@api-typed/logger": "file:packages/logger",
    "@api-typed/message-queue": "file:packages/message-queue",
    "@api-typed/standard-app": "file:packages/standard-app",
    "@api-typed/testing": "file:packages/testing",
    "@api-typed/typeorm-module": "file:packages/typeorm-module",
    "api-typed": "file:packages/api-typed"
  },
  "devDependencies": {
    "@types/express": "4.17.11",
    "@types/find-package-json": "1.2.0",
    "@types/glob": "7.1.3",
    "@types/jest": "^26.0.22",
    "@types/lodash.get": "4.4.6",
    "@types/lodash.merge": "4.6.6",
    "@types/mkdirp": "1.0.1",
    "@types/node": "^14.14.37",
    "@types/serve-favicon": "2.5.2",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "class-transformer": "*",
    "class-validator": "*",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "lerna": "^4.0.0",
    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "routing-controllers": "*",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typedi": "*",
    "typeorm": "^0.2.32",
    "typescript": "^4.2.3"
  }
}
